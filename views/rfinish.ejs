        <th>가격</th>
    </tr>
    <%
    console.log("@@@@@@@@@@@@@@@@@@@@@@@@@", page, page_num);

    for(var i = (page * page_num) - page_num; i < (page * page_num); i++) {
    if(i > length){
        i++;
    }else{
        var data = rows[i]
    %>
    <tr>
        <td id="number"><%=i+1%></td>
        <td><%=data.recompany%></td>
        <td><%=data.writer%></td>
        <td><%=data.title%></td>
        <td><%=data.gearcompany%></td>
        <td><%=data.codenum%></td>
        <td><%=data.codeserial%></td>
        <td><%=data.startday%></td>
        <td><%=data.endday%></td>
        <td><%=data.clientsym%></td>
        <td><%=data.enduser%></td>
        <td><%=data.place%></td>
        <td><%=data.price%></td>
    </tr>
    <% }
    }
    %>
    <tr>
        <td colspan="20">
            <%
            for(var j = 0; j < rows.length / page_num; j++){
            %>
                [<a href="/board/page/<%= j + 1 %>"><%= j + 1 %></a>]
            <%
            }
            %>
        </td>
    </tr>
